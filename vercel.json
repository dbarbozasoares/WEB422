{
  "version": 2,
  "builds": [
    {
      "src": "Assignment_1/server.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "Assignment_1/public/**",
          "Assignment_1/views/**",
          "Assignment_1/modules/**"
        ]
      }
    },
    {
      "src": "Assignment_3/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "^/$",
      "dest": "/assignment_3/pages/index",
      "headers": {
        "Cache-Control": "no-store"
      }
    },
    {
      "src": "/about",
      "dest": "/assignment_3/pages/about",
      "headers": {
        "Cache-Control": "no-store"
      }
    },
    {
      "src": "/api/listings",
      "dest": "Assignment_1/server.js",
      "headers": {
        "Cache-Control": "no-store"
      }
    },
    {
      "src": "/api/listings/(.*)",
      "dest": "Assignment_1/server.js",
      "headers": {
        "Cache-Control": "no-store"
      }
    },
    {
      "src": "/(.*)",
      "dest": "Assignment_1/server.js",
      "headers": {
        "Cache-Control": "no-store"
      }
    }
  ]
}
