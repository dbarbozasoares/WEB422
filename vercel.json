{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node",
        "config": {
          "includeFiles": ["public/**"]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "Assignment_1/server.js",
        "headers": {
          "Cache-Control": "no-store"
        }
      }
    ]
  }
  